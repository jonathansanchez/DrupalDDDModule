{% if data|length %}
    <table>
        <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
        </thead>
        <tbody>
        {% for value in data %}
            <tr>
                <td>{{ value.id }}</td>
                <td>{{ value.name }}</td>
                <td>{{ value.email }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% else %}
    <div>
        <strong>No data.</strong>
    </div>
{% endif %}